{% trans_default_domain 'FOSUserBundle' %}

<div class="fos_user_user_show"></div>


<table class="table">

    <tr><th>{{ 'profile.show.username'|trans }}</th><td>{{ user.username }}</td></tr>
    <tr><th>{{ 'profile.show.email'|trans }}</th><td>{{ user.email }}</td></tr>
    <tr><th>Firstname</th><td>{{ user.firstname }}</td></tr>
    <tr><th>Lastname</th><td>{{ user.lastname }}</td></tr>
</table>


